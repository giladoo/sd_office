<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
    <t t-name="sd_office.console_template" owl="1">
        <div class="container">
            <div class="row mt-4">
                <DataCards name="state.user.date"
                           onClick="() => false"
                           value="state.user.name" />

                <VisitorsCard name="state.visitors.name"
                              onClick="() => onClickDataCard(state.visitors.id)"
                              visits="state.visitors.visits" />

            </div>
        </div>

    </t>
</templates>